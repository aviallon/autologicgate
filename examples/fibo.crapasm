; Fibonacci
byte    u_n     @0x1e
byte    u_n_m1   @0x1f
;byte    u_np2   @0x20

start:      MOV u_n_m1, #0x00
            MOV u_n, #0x01
loop:       MOV A, u_n
            MOV U0, #0x00
            ADD U0, u_n
            ADD U0, u_n_m1
            MOV u_n, U0
            MOV u_n_m1, A
            JMP loop
            HALT
